<%- include('../partials/header') %>
<h2>Add New Event</h2>
<form action="/events" method="POST">

  <label>Event Name:</label>
  <input type="text" name="eventName" required /><br />

  <label>Busker Name:</label>
  <input type="text" name="buskerName" required /><br />

  <label>Date:</label>
  <input type="date" name="eventDate" required /><br />

  <label>Location:</label>
  <input type="text" name="location" required /><br />

  <label>Event Description</label>
  <input type="text" name="eventDescription" required /> <br />


  <button type="submit">Add Event</button>
  <button type="reset">Cancel</button> <br />

</form>
<%- include('../partials/footer') %>