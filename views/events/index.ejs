<%- include('../partials/header') %>

<h2>Upcoming Events</h2>
<table border="1">
  <thead>
    <tr>
      <th>Event Name</th>
      <th>Busker Name</th>
      <th>Location</th>
      <th>Date</th>
      <th>Event Description</th>

    </tr>
  </thead>
  <tbody>
    <% events.forEach(e => { %>
    <tr>
      <td> <a href="/events/<%=e.id %>"><%= e.eventName %></a>
      <td><%= e.buskerName %></td>
      <td><%= e.location %></td>
      <td><%= dayjs(e.eventDate).format("DD-MMM-YYYY") %></td>
      <td><%= e.eventDescription %></td </tr>

      <% }) %>
  </tbody>
</table>



<%- include('../partials/footer') %>